{
	"name": "Open in Habra.",
	"description" : "Extension for open posts from habr.com in habra.js.org",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["webRequest", "*://*.habr.com/*", "webRequestBlocking"],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "habra.png"
	}
}
